{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/gen",
    "template": "node_modules/minami"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "outputSourceFiles": true,
    "path": "ink-docstrap",
    "theme": "lumen",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "systemName": "Futz"
  }
}
